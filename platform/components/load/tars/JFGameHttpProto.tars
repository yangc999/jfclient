module JFGame
{		
	//用户标识
	struct TUid
	{
		0 require	long    lUid;    //用户ID
		1 require   string  sToken;	 //登录token值
	};

	//请求返回HTTP协议包体数据
	struct THttpPackage
	{
		0 require	int 		iVer;			// 版本号
		1 require	int			iSeq;			// 序列号
		2 require	TUid		stUid;			// 用户标识
		3 optional	vector<byte> vecData;		// 命令对应的协议数据
	};	
	
};
